{
	"expo": {
		"name": "batler",
		"slug": "batler",
		"version": "0.6.0",
		"scheme": "batler",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"bundleIdentifier": "is.pvin.batler",
			"supportsTablet": true,
			"infoPlist": {
				"UIBackgroundModes": ["audio"]
			}
		},
		"android": {
			"package": "is.pvin.batler",
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#FFFFFF"
			}
		}
	}
}
